        "#変数の定義\n",
        "var=30\n",
        "#3の倍数かつ5の倍数の時FizzBuzz\n",
        "if var%3 == 0 and var%5 == 0 :\n",
        "  print(\"FizzBuzz\")\n",
        "  #3の倍数の時Fizz\n",
        "elif var%3 == 0 :\n",
        "  print(\"Fizz\")\n",
        "  #5の倍数の時Buzz\n",
        "elif var%5 == 0 :\n",
        "  print(\"Buzz\")\n",
        "  #それ以外の時varを表示\n",
        "else:\n",
        "  print(var)"
